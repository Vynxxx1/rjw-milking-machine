<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <HediffDef>
        <defName>LM_MilkingHediff</defName>
        <label>Being milked</label>
        <description>Currently being milked by a milking machine, drained of all their tastier juices.</description>
        <isBad>false</isBad>
        <initialSeverity>0.05</initialSeverity>
        <maxSeverity>1</maxSeverity>
        <stages>
            <li>
                <minSeverity>0.05</minSeverity>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <setMax>0.7</setMax>
                    </li>
                    <li>
                        <capacity>Moving</capacity>
                        <setMax>0.5</setMax>
                    </li>
                </capMods>
            </li>
        </stages>
        <hediffClass>MilkingMachine.MilkingHediff</hediffClass>
        <defaultLabelColor>(1.0, 1.0, 1.0)</defaultLabelColor>
    </HediffDef>

</Defs>